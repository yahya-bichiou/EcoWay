{% extends 'base.html.twig' %}

{% block title %}Participant{% endblock %}

{% block body %}
    <h1>Participant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ participant.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ participant.nom }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ participant.age }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ participant.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ participant.email }}</td>
            </tr>
            {#<tr>
                <th>evenement</th>
                <td>{{ participant.evenement }}</td>
            </tr>#}
        </tbody>
    </table>

    <a href="{{ path('app_participant_indexback') }}">back to list</a>
    {{ include('participant/_delete_form.html.twig') }}
{% endblock %}
